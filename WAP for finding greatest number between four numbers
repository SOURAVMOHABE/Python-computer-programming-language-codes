Number_1 = int(input("Enter the first number : \n"))
Number_2 = int(input("Enter the second number : \n"))
Number_3 = int(input("Enter the third number : \n"))
Number_4 = int(input("Enter the fourth number : \n"))
if(Number_1>Number_4):
    N1 = Number_1
else:
    N1 = Number_4

if(Number_2>Number_3):
    N2 = Number_2
else:
    N2 = Number_3

if(N1>N2):
    print(N1, "is greater")
else:
    print(N2, "is greater")
